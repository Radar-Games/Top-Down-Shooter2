    playerLevel += 1;
    playerExp = playerExp - playerMaxExp;
    playerMaxExp += 100;
}

if(keyboard_check_pressed(ord('9'))){
    playerExp += 10;
}

// Exiting Level

if(keyboard_check_pressed(vk_escape)){
    room = rm_menu;
}

// Saving

if(file_exists('AGAHBTSDFATJDg^5isdkadASdgfSAdASIdyasdasasyd.sav')){
    file_delete('AGAHBTSDFATJDg^5isdkadASdgfSAdASIdyasdasasyd.sav');
    ini_open('AGAHBTSDFATJDg^5isdkadASdgfSAdASIdyasdasasyd.sav');
    ini_write_real('Level Indicaters', 'Level', playerLevel);
    ini_write_real('Level Indicaters', 'Experience', playerExp);
    ini_write_real('Level Indicaters', 'MaxExperience', playerMaxExp);
    ini_close();
}else if(!file_exists('AGAHBTSDFATJDg^5isdkadASdgfSAdASIdyasdasasyd.sav')){
    ini_open('AGAHBTSDFATJDg^5isdkadASdgfSAdASIdyasdasasyd.sav');
